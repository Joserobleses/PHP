<VirtualHost *:80>
    # ...
    DocumentRoot "C:/xamppPHP8/htdocs/symfoplaces/public"
    DirectoryIndex index.php
    <Directory "C:/xamppPHP8/htdocs/symfoplaces/public">
        AllowOverride All
        Require all granted
         <IfModule mod_rewrite.c>
            Options -MultiViews
            RewriteEngine On
            RewriteCond %{REQUEST_FILENAME} !-f
            RewriteRule ^(.*)$ index.php [QSA,L]
         </IfModule>
    </Directory>
</VirtualHost>

<VirtualHost *:80>
    DocumentRoot "C:/xamppPHP8/htdocs"
    ServerName localhost
</VirtualHost>